
public class HelloWorld {
	private int num; 
	private String greet;
	
	
	
	public HelloWorld() {
		this.greet = "Hello World!";
	}



	public int getNum() {
		return num;
	}



	public void setNum(int num) {
		this.num = num;
	}



	public String getGreet() {
		return greet;
	}



	public void setGreet(String greet) {
		this.greet = greet;
	}
	
}
