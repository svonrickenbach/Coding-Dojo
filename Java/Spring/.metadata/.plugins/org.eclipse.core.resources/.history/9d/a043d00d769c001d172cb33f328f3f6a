
public class Driver extends Car {

	public Driver() {
		super();
	}
	
	public void driving() {
		setGasLevel(-1);
		System.out.println("You drive the car. \n Gas remaining: " + getGasLevel() + "");
	}
	
	public void booster() {
		setGasLevel(-3);
		System.out.println(getGasLevel());
	}
	
	public void refuel() {
		setGasLevel(2);
		System.out.println(getGasLevel());
	}
}


